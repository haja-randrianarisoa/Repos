{
  "name": "amazon-e2e-tests",
  "description": "Test project using Protractor and jasmine",
  "author": "Amazon",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run webdriver-update",
    "webdriver-update": "webdriver-manager update",
    "clean": "del-cli Reports/Recent/*",
    "mkdir": "make-dir Reports/Recent --mode=0766",
    "prelocaltest": "npm run backupreport && npm run clean && npm run mkdir",
    "localtest": "dotenv -e ./local.env protractor Configurations/conf_local.js",
    "pretest": "npm run clean && npm run mkdir",
    "test": "protractor Configurations/conf_local.js",
    "backupreport": "node ./Resources/backupReport.js",
    "showreport": "opener ./Reports/Recent/report.html",
    "updatetestdata": "node ./Resources/convertExcelToJson.js",
    "emailablereport": "node ./Resources/emailableReports.js"
  },
  "devDependencies": {
