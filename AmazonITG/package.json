    "emailjs": "^3.3.0",
    "encryptjs": "^1.1.1",
    "grunt": "^1.3.0",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-execute": "^0.2.2",
    "grunt-mkdir": "^1.0.0",
    "grunt-protractor-runner": "^5.0.0",
    "grunt-run-executables": "~0.0.2",
    "jasmine": "^3.6.1",
    "jasmine-custom-message": "^0.8.1",
    "jasmine-reporters": "^2.3.2",
